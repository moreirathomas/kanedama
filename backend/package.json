{
  "scripts": {
    "build": "tsc --build ./tsconfig.json",
    "build:watch": "tsc --build -w ./tsconfig.json",
    "start": "node -r dotenv/config ./dist dotenv_config_path=.env.dev",
    "start:node": "nodemon -r dotenv/config ./dist/index.js dotenv_config_path=.env.dev",
    "prestart": "npm run build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "test:e2e": "test/e2e/api.sh",
    "test:goldenmaster": "testdata/goldenmaster/snapshot.sh"
  },
  "dependencies": {
    "fastify": "4.0.3",
    "knex": "2.1.0",
    "pg": "8.7.3"
  },
  "devDependencies": {
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "dotenv": "16.0.3",
    "eslint": "8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.18",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  }
}
